    "automock": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "pug"
    ],
    "moduleDirectories": [
      "<rootDir>/src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|scss)$": "<rootDir>/src/__tests__/support/fileMock.js",
      "^ui/store/actions/runSearch(.*)$": "<rootDir>/src/ui/store/actions/runSearch.client.js"
    },
    "transform": {
      "^.+\\.pug$": "<rootDir>/src/__tests__/support/pug-jest-loader",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__tests__/support/"
    ],
    "setupFiles": [
      "./config/dotenv.js",
      "./src/__tests__/support/setupFetch.js"
    ],
